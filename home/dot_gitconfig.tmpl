[user]
	email = {{ .personalEmail | quote }}
	name = Julian POIDEVIN

[color]
	ui = auto

[core]
	editor = code-insiders
	whitespace = trailing-space,space-before-tab

[fetch]
	fsckobjects = false
	prune = true

[status]
	submoduleSummary = true

[includeIf "gitdir:~/repo/"]
    path = ~/repo/.gitconfig
